/* pages/my/my.wxss */
.mycon {
  background-color: rgba(223, 220, 220);
  height: 100vh;
}
.head {
  width: 100vw;
  height: 320rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-image: url('https://i0.hdslb.com/bfs/article/60addff6ba335b57110a408199e5174b8210d5bc.png@942w_533h_progressive.webp');
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}
.loging {
  background-color: rgba(223, 220, 220, 0.8);
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 200rpx;
}
text {
  position: absolute;
  color: rgb(124, 124, 124);
  font-weight: 700;
}
.bottom {
  transform: translateY(-30rpx);
  display: flex;
  flex-direction: column;
  align-items: center;
}
.conall {
  width: 90vw;
  height: 110rpx;
  background-color: rgb(255, 255, 255);
  margin: 10rpx 0rpx;
  border-radius: 10rpx;
  padding: 10rpx;
}
.numall {
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.numlist {
  width: 150rpx;
  display: flex;
  flex-direction: column;
  font-size: small;
  text-align: center;
}
.ordertop {
  height: 60rpx;
  text-align: left;
  line-height: 60rpx;
}
.ordercon {
  height: 140rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.orderimg {
  width: 60rpx;
}
.iconcon {
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.adm {
  height: 46rpx;
  line-height: 46rpx;
  font-size: medium;
}